{
	"name": "konstframjandet-triennalen",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm run dev:watch",
		"dev:watch": "LOG_GRAPHQL=false next dev & npm run barrel:watch & graphql-codegen --watch",
		"dev:emails": "npx mailing",
		"build": "next build && next-sitemap",
		"codegen": "graphql-codegen",
		"start": "next start",
		"lint": "next lint",
		"preview": "npm run build && npm run start",
		"barrel": "node ./lib/scripts/barrel",
		"barrel:watch": "watch 'node ./lib/scripts/barrel' ./components",
		"postbuild": "next-sitemap",
		"test": "tsx --tsconfig ./tsconfig.json ./lib/scripts/test.tsx",
		"migrate:clean": "datocms environments:destroy migration ; datocms environments:fork main migration",
		"migrate:errors": "tsx ./lib/scripts/migration/errors.ts",
		"migrate:all": "npm run migrate:clean && tsx ./lib/scripts/migration/all.ts",
		"migrate:news": "tsx ./lib/scripts/migration/news.ts",
		"migrate:projects": "tsx ./lib/scripts/migration/projects.ts",
		"migrate:collaborator": "tsx ./lib/scripts/migration/collaborator.ts",
		"migrate:abouts": "tsx ./lib/scripts/migration/abouts.ts",
		"migrate:test": "datocms environments:fork main dev ; tsx ./lib/scripts/migration/projects.ts"
	},
	"exports": {
		"lib/*": "./lib",
		"components/*": "./components",
		"graphql/*": "./graphql",
		"/*": "./*"
	},
